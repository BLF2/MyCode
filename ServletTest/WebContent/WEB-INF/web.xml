<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>ServletA</servlet-name>
    <servlet-class>ServletP.ServletClass</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletA</servlet-name>
    <url-pattern>/ServletP/ServletClass</url-pattern>
  </servlet-mapping>

<servlet>
	 <servlet-name>ServletF</servlet-name>
	<servlet-class>ServletP.ServletFront</servlet-class>
</servlet>
<servlet-mapping>
	 <servlet-name>ServletF</servlet-name>
    <url-pattern>/ServletP/ServletFront</url-pattern>
</servlet-mapping>
</web-app>